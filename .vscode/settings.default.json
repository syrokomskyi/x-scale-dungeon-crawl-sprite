{
  "typescript.tsdk": "node_modules/typescript/lib",
  "biome.lsp.bin": "biome",
  "biome.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "workbench.iconTheme": "fira-code-material-minimal",
  "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.unicodeHighlight.allowedLocales": {
    "ru": true,
    "uk": true
  },
  "editor.accessibilitySupport": "off",
  "explorer.confirmDragAndDrop": false,
  "redhat.telemetry.enabled": true,
  "editor.unicodeHighlight.allowedCharacters": {
    "і": true,
    "І": true
  },
  "git.openRepositoryInParentFolders": "never",
  "typescript.tsserver.watchOptions": {},
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "dart.openDevTools": "flutter",
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": true,
  "[env]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "files.associations": {
    ".env": "env",
    "*.css": "tailwindcss"
  },
  "hidefiles.configurationType": "local",
  "hidefiles.globalConfig": {
    "profiles": [
      {
        "hidden": [
          "**/__pycache__",
          "**/__init__.py"
        ],
        "name": "Python",
        "description": "",
        "detail": "",
        "peek": []
      },
      {
        "name": "Default",
        "detail": "",
        "description": "Test",
        "hidden": [
          ".vscode/"
        ]
      },
      {
        "hidden": [
          "**/.vscode/",
          "**/node_modules/",
          "**/.turbo/",
          "**/dist/",
          "**/build/",
          "**/.wrangler/"
        ],
        "name": "MonoRepo",
        "description": "",
        "detail": "",
        "peek": []
      }
    ]
  },
  "hidefiles.selectedProfile": "Show All Files",
  "hide-files.files": [],
  "explorer.confirmPasteNative": false,
  "hidefiles.version": "2.0.0",
  "dart.showDebuggerNumbersAsHex": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "statusBar.background": "#7C21D7",
    "statusBar.debuggingBackground": "#7C21D7",
    "statusBar.noFolderBackground": "#7C21D7",
    "statussBar.prominentBackground": "#7C21D7",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveForeground": "#000000cc",
    "titleBar.activeBackground": "#fc9d26",
    "titleBar.inactiveBackground": "#fc9d26cc"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "GitHub Dark Default",
  "git.enableSmartCommit": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  }
}
